// 测试 if 语句内存

fn test_if(n: bigint) -> bigint {
    if n <= 0B {
        return 0B;
    }
    return 1B;
}

fn main_test() {
    print("=== If Statement Test ===");
    bigint_debug_stats();

    let r: bigint = test_if(5B);
    print(r);

    bigint_debug_stats();
}

main_test();
print("Final:");
bigint_debug_stats();
