// 测试运算符重载

class Vector {
    x: int;
    y: int;

    fn __add__(other: Vector) -> int {
        return self.x + other.x + self.y + other.y;
    }

    fn __eq__(other: Vector) -> int {
        if self.x == other.x {
            if self.y == other.y {
                return 1;
            }
        }
        return 0;
    }
}

let v1: Vector = Vector();
v1.x = 10;
v1.y = 20;

let v2: Vector = Vector();
v2.x = 30;
v2.y = 40;

let sum: int = v1 + v2;
print(sum);

let eq: int = v1 == v2;
print(eq);

v2.x = 10;
v2.y = 20;
let eq2: int = v1 == v2;
print(eq2);
