// 测试回调函数

// 加法函数 - 将作为回调传递
fn add(a: int, b: int) -> int {
    return a + b;
}

// 乘法函数
fn mul(a: int, b: int) -> int {
    return a * b;
}

// 平方函数
fn square(x: int) -> int {
    return x * x;
}

// 测试 test_callback
let r1: int = test_callback(add, 10, 20);
print(r1);  // 应该输出 30

let r2: int = test_callback(mul, 6, 7);
print(r2);  // 应该输出 42

// 测试 map_int
let r3: int = map_int(square, 5);
print(r3);  // 应该输出 25

print(999);
