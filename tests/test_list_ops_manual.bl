
print("Testing List Operations");

let l: list<int> = [1, 2];

// Test push/append
// Try push first as it's the runtime name
l.push(3);
print(l); // Expect [1, 2, 3]

// Test len
print(l.len()); // Expect 3

// Test pop
let popped: int = l.pop();
print(popped); // Expect 3
print(l); // Expect [1, 2]

// Test get/set
print(l[0]);
l[0] = 100;
print(l[0]);
