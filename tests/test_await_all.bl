// 测试 await all

async fn task1() -> int {
    return 10;
}

async fn task2() -> int {
    return 20;
}

async fn task3() -> int {
    return 30;
}

// 并发等待多个任务
let results: (int, int, int) = await all {
    task1(),
    task2(),
    task3()
};

print(results);
print(results[0]);
print(results[1]);
print(results[2]);

print(999);
