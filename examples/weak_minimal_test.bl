// 最小化 AOT 测试 - 不使用函数调用

class WeakNode {
    value: int;
    next: WeakNode;
    prev: weak WeakNode;
}

print("Step 1: Creating n1\n");
let n1: WeakNode = WeakNode(1, none, none);
print("Step 2: Creating n2\n");
let n2: WeakNode = WeakNode(2, none, none);
print("Step 3: Assigning n1.next = n2\n");
n1.next = n2;
print("Step 4: Assigning n2.prev = n1 (weak)\n");
n2.prev = n1;
print("Step 5: Done!\n");
