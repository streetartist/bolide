// å¤§æ•°é˜¶ä¹˜
fn bigint_factorial(n: bigint) -> bigint {
    if n <= 1B {
        return 1B;
    }
    return n * bigint_factorial(n - 1B);
}

// å¤§æ•° Fibonacci
fn bigint_fib(n: int) -> bigint {
    if n <= 1 {
        return 1B;
    }
    let a: bigint = 0B;
    let b: bigint = 1B;
    for i in range(n) {
        let temp: bigint = a + b;
        a = b;
        b = temp;
    }
    return b;
}

print("50! =");
print(bigint_factorial(50B));

print("fib(100) =");
print(bigint_fib(100));

