print("");
print("â–¶ 19. åµŒå¥—å¼‚æ­¥è°ƒç”¨ Nested Async");
print("â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€");

async fn inner_async(n: int) -> int {
    return n + 10;
}

async fn outer_async(n: int) -> int {
    let f: future = inner_async(n);
    let result: int = await f;
    return result * 2;
}

let nested_future: future = outer_async(5);
let nested_result: int = await nested_future;
print("outer_async(5) = (5+10)*2 =");
print(nested_result);

