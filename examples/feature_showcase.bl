class Point {
    x: int;
    y: int;

    fn distance() -> int {
        return self.x * self.x + self.y * self.y;
    }

    fn move_by(dx: int, dy: int) {
        self.x = self.x + dx;
        self.y = self.y + dy;
    }
}

let p: Point = Point(3, 4);
print("Point(3, 4)");
print("distance^2 = ");
print(p.distance());

p.move_by(1, 1);
print("After move_by(1, 1): x=");
print(p.x);
print("y=");
print(p.y);

