let big_a: bigint = 123456789B;
let big_b: bigint = 987654321B;

let bigint_t1: future = spawn thread_bigint_square(big_a);
let bigint_t2: future = spawn thread_bigint_square(big_b);

print("123456789^2 =");
print(join(bigint_t1));
print("987654321^2 =");
print(join(bigint_t2));

