fn pipeline_stage1(x: int) -> int {
    return x * 2;
}

fn pipeline_stage2(x: int) -> int {
    return x + 100;
}

fn pipeline_stage3(x: int) -> int {
    return x / 2;
}

let input: int = 50;

// Stage 1
let s1: future = spawn pipeline_stage1(input);
let mid1: int = join(s1);
print("Stage 1 (x*2):");
print(mid1);

// Stage 2
let s2: future = spawn pipeline_stage2(mid1);
let mid2: int = join(s2);
print("Stage 2 (+100):");
print(mid2);

// Stage 3
let s3: future = spawn pipeline_stage3(mid2);
let final_result: int = join(s3);
print("Stage 3 (/2):");
print(final_result);

